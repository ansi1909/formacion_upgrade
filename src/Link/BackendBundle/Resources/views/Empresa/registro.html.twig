{% extends '::admin.html.twig'%}
{% block body %}
<br>
<div class="row">
	<div class="col-sm-16">
		<div class="card">
			<div class="card-header">
                <div class="card-title">
                    <h6>Registrar Empresa</h6>
                </div>
            </div>
           	<div class="card-block">
                <div class="container_wizard wizard-thin">
                	<div class="stepwizard">
                		<div class="stepwizard-row setup-panel d-flex">
            				<div class="stepwizard-step col">
                                <a href="#det0" class="btn bttn__fndo pass">
                                    <span class="btn-round">1</span>
                                    <p>Datos Básicos</p>
                                </a>
                            </div>
                            <div class="stepwizard-step col">
                                <a href="#det1" class="btn btn-secondary pass disabled">
                                    <span class="btn-round">2</span>
                                    <p>Bienvenida</small></p>
                                </a>
                            </div>
                            <div class="stepwizard-step col">
                                <a href="#det2" class="btn btn-secondary pass disabled">
                                    <span class="btn-round">3</span>
                                    <p>Confirmar</small></p>
                                </a>
                            </div>
                		</div>
                	</div>
                	<form action="" class="container-fluid p-0">
                		<div class="row setup-content" id="det0">	
                			<div class="col-sm-16">
                				<div class="row">
                                    <div class="col-sm-16">
                                        <h3 class="mb-2">Paso1</h3>
                                    </div>
                                </div>
                                <br>
                                <div class="row">
                                	<div class="col-sm-16 col-md-8 col-lg-6">
                                		<div class="form-group">
                                            <label for="texto" class="col-2 col-form-label">{{ 'Nombre Empresa'|trans }}</label>
                                            <div class="col-14">
                                                <input type="text" class="form-control form_sty1" id="texto" placeholder="Usuario">
                                                <span class="fa fa-industry"></span>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="texto" class="col-2 col-form-label">{{ 'Correo'|trans }}</label>
                                            <div class="col-14">
                                                <input type="text" class="form-control form_sty1" id="texto" placeholder="Usuario">
                                                <span class="fa fa-user"></span>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="Correo" class="col-2 col-form-label">Teléfono</label>
                                            <div class="col-14">
                                                <input type="email" class="form-control form_sty3" id="Correo" placeholder="Correo">
                                                <span class="fa fa-envelope"></span>
                                            </div>
                                        </div>
                                	</div>
                                	<div class="col-sm-16 col-md-8 col-lg-6">
                                		
                                	</div>
                                </div>
                			</div>
                		</div>
                	</form>
                </div>
            </div>
		</div>
	</div>
</div>
{% endblock %}

